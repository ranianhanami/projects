ntent/random-forest-regressor.pkl', 'rb'))\n","  lag = 3\n","\n","  predicted_date =  datetime.datetime.strptime(date, \"%Y-%m-%d\").date()\n","  filtered_data = data[(data['store'] == store) & (data['item'] == item)]\n","  gap = predicted_date - date_limit\n","  gap_days = gap.days\n","  lag = filtered_data['sales'][-lag:]\n","\n","  res = np.array([store, item])\n","  res = [np.append(res, lag.values)]\n","\n","  y_pred = model.predict(res)[0]\n","  return y_pred"],"metadata":{"id":"PVxpDBakIyxH","executionInfo":{"status":"ok","timestamp":1655777375689,"user_tz":-420,"elapsed":10,"user":{"displayName":"Raniah Nur Hanami","userId":"11917664539767615267"}}},"execution_count":4,"outputs":[]},{"cell_type":"markdown","source":["## Use model"],"metadata":{"id":"YYvBHToZ1_6R"}},{"cell_type":"code","source":["date = '2017-01-04'\n","store = 3\n","item = 1"],"metadata":{"id":"gsm_hkJX2nu4"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["random_forest_regressor(date,store,item)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"V5zHC3eW5JDn","executionInfo":{"status":"ok","timestamp":1655259688266,"user_tz":-420,"elapsed":1127,"user":{"displayName":"Raniah Nur Hanami","userId":"11917664539767615267"}},"outputId":"87094d26-bcee-4b8f-ed1b-91a583e4cfc1"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["26.5"]},"metadata":{},"execution_count":21}]}]}